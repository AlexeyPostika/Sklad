<Window x:Class="PosBackup.FlexControls.ShopBackups"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"        
        Title="FlexWindow" Height="300" Width="300"
        SizeToContent="WidthAndHeight"
        WindowStartupLocation ="CenterScreen"
        ResizeMode="NoResize"  
        x:Name="WindowName"
        Loaded="WindowName_Loaded"        
        >
    <Grid>
        <DataGrid x:Name="DataGridBackup"
                  Grid.Column="2"
                  Grid.Row="2"
                  AutoGenerateColumns="True"
                  IsReadOnly="True"
                  CanUserAddRows="False"
                  HorizontalScrollBarVisibility="Auto"
                  VerticalScrollBarVisibility="Auto" MouseDoubleClick="DataGridBackup_MouseDoubleClick"
                  
                  >
        </DataGrid>
    </Grid>
</Window>
