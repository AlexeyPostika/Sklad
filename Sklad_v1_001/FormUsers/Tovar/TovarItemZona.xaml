<UserControl x:Class="Sklad_v1_001.FormUsers.Tovar.TovarItemZona"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:res="clr-namespace:Sklad_v1_001.Properties"
             xmlns:ToolBar="clr-namespace:Sklad_v1_001.Control.ToolBar"
             xmlns:ImageList="clr-namespace:Sklad_v1_001.Control.FlexImage"
             mc:Ignorable="d"   
             Style="{DynamicResource UsersControlWidth}"
             d:DesignHeight="918" Width="1462" Loaded="UserControl_Loaded" x:Name="PageTovarZona">
    <Grid>
        
            <ImageList:ImageListPage  HorizontalAlignment="Left" Width="1462" x:Name="Imagelist"
                                      Image="{Binding Path=PhotoImage, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}"
                                      ListImageControl="{Binding ElementName=PageTovarZona, Path=ListImage, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}"/>
        
    </Grid>
</UserControl>
