        <Grid HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="234" Background="#FFC0C000"/>
        <!--грид горизонтальное меню-->
        <Grid HorizontalAlignment="Left" Height="818" Margin="0,100,0,0" VerticalAlignment="Top" Width="234" Background="#FF292933">
            <Grid HorizontalAlignment="Left" Height="117" Margin="10,10,0,0" VerticalAlignment="Top" Width="214">
                <Image HorizontalAlignment="Left" Height="76" VerticalAlignment="Top" Width="76" Source="/Sklad_v1_001;component/Icone/admin2.png"/>
