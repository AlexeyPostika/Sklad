        </Grid>
        <!--вертикальное меню-->
        <Grid HorizontalAlignment="Left" Height="100" Margin="234,0,0,0" VerticalAlignment="Top" Width="1034" Background="#FF292933"/>
        <!--грид основной контейнер-->
        <Grid Height="818" Margin="234,100,0,0" VerticalAlignment="Top" Background="#FF292933">
            <TextBlock HorizontalAlignment="Left" Margin="-155,-45,0,0" TextWrapping="Wrap" Text="KLAD" VerticalAlignment="Top" Height="39" RenderTransformOrigin="0.079,0.407" Width="116" FontSize="32" FontWeight="Bold" FontFamily="Castellar"/>
            <Frame x:Name="Docker1" Margin="0,1" NavigationUIVisibility="Hidden" HorizontalAlignment="Left"  VerticalAlignment="Top"/>
        </Grid>
        <Image HorizontalAlignment="Left" Height="50" VerticalAlignment="Top" Width="51" RenderTransformOrigin="-4.9,-2.62" Source="/Sklad_v1_001;component/Icone/Sklad.png" Margin="103,10,0,0"/>
        <TextBlock HorizontalAlignment="Left" Margin="12,-7,0,0" TextWrapping="Wrap" Text="S" VerticalAlignment="Top" Height="95" Width="91" FontSize="95" FontWeight="Bold" FontFamily="Castellar" TextAlignment="Center" RenderTransformOrigin="0.835,0.484"/>

    </Grid>
</UserControl>
