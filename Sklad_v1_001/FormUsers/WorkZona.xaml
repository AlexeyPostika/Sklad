                <TextBlock x:Name="Dolwnost" HorizontalAlignment="Left" Margin="81,90,0,0" TextWrapping="Wrap" Text="----------------" VerticalAlignment="Top" Width="123" Foreground="#FFFBF6F6" Height="22" FontSize="16" FontWeight="Bold" TextAlignment="Center" RenderTransformOrigin="0.512,-1.182"/>
            </Grid>
            <TextBlock x:Name="LastNamePoln" HorizontalAlignment="Left" Margin="92,43,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="123" Foreground="#FFFBF6F6" Height="22" FontWeight="Bold" TextAlignment="Center" RenderTransformOrigin="0.512,-1.182" FontSize="16"><Run Language="ru-ru" Text="-------------------"/></TextBlock>
            <Image HorizontalAlignment="Left" Height="40" Margin="0,132,0,0" VerticalAlignment="Top" Width="40" Source="/Sklad_v1_001;component/Icone/Menu/product_80px.png"/>
            <Image HorizontalAlignment="Left" Height="40" Margin="0,177,0,0" VerticalAlignment="Top" Width="40" Source="/Sklad_v1_001;component/Icone/Menu/trolley_40px.png"/>
            <Image HorizontalAlignment="Left" Height="40" Margin="0,222,0,0" VerticalAlignment="Top" Width="40" Source="/Sklad_v1_001;component/Icone/Menu/buy_40px.png"/>
            <Image HorizontalAlignment="Left" Height="40" Margin="0,398,0,0" VerticalAlignment="Top" Width="40" Source="/Sklad_v1_001;component/Icone/Menu/cash_register_80px.png" RenderTransformOrigin="0.475,0.025"/>
