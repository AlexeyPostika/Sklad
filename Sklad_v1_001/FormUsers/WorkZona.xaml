            <Button x:Name="ListTovar" Content="Товар" HorizontalAlignment="Left" Margin="45,132,0,0" VerticalAlignment="Top" Width="177" Height="40" Background="#FF292933" FontSize="16" FontWeight="Bold" Foreground="#FFF9F8F8" BorderBrush="#FF292933" Click="ListTovar_Click"/>
            <Button x:Name="PrixodTovara" Content="Приход" HorizontalAlignment="Left" Margin="45,177,0,0" VerticalAlignment="Top" Width="177" Height="40" Background="#FF292933" FontSize="16" FontWeight="Bold" Foreground="#FFF9F8F8" BorderBrush="#FF292933" Click="PrixodTovara_Click"/>
            <Button x:Name="RasxodTovara" Content="Расход" HorizontalAlignment="Left" Margin="45,222,0,0" VerticalAlignment="Top" Width="177" Height="40" Background="#FF292933" FontSize="16" FontWeight="Bold" Foreground="#FFF9F8F8" BorderBrush="#FF292933"/>
            <Button x:Name="Kategorii" Content="Категории" HorizontalAlignment="Left" Margin="45,267,0,0" VerticalAlignment="Top" Width="177" Height="40" Background="#FF292933" FontSize="16" FontWeight="Bold" Foreground="#FFF9F8F8" BorderBrush="#FF292933" Click="Kategorii_Click"/>
