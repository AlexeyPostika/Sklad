<dialog:DialogWindow x:Class="Sklad_v1_001.Report.FlexDocumentWindows"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Sklad_v1_001.Report"
      xmlns:dialog="clr-namespace:Sklad_v1_001.Control"
      mc:Ignorable="d"       
      WindowStartupLocation ="CenterScreen"
      ResizeMode="NoResize"
      Height="800" Width="800"
      Title="FlexDocumentWindows"
      x:Name="page">

    <Grid>
        <DocumentViewer x:Name="documentViewer" HorizontalAlignment="Left" VerticalAlignment="Top"
                        Document="{Binding ElementName=page, Path=DocumentXps, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
    </Grid>
</dialog:DialogWindow>
