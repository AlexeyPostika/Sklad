    <Style x:Key="IconArrow" TargetType="ContentControl">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ContentControl">

                    <Canvas Width="4" Height="4">
                        <Canvas>
                            <Canvas.RenderTransform>
                                <TranslateTransform X="0" Y="-20"/>
                            </Canvas.RenderTransform>
                            <Canvas>
                                <Path Fill="#FF000000" Data="M 2.1855377 20 4 21.995049 2.1781545 24 h -0.75647 L 2.8761097 22.280945 H 0 v -0.56189 H 2.8761097 L 1.4364505 20 Z"/>
                            </Canvas>
