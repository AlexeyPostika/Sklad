<ROOT>
  <LOG>
    <TYPE>DEBUG</TYPE>
    <CLEARTIME>30</CLEARTIME>
  </LOG>
  <FREERESOURCE_AUTO>ON</FREERESOURCE_AUTO>
  <TRIPLEACK>ON</TRIPLEACK>
  <DEVICES>
    <DEVICE>
      <TYPE>TERMINAL</TYPE>
      <CONNECTION>
        <TYPE>COM</TYPE>
        <PORT>COM6</PORT>
        <BAUDRATE>115200</BAUDRATE>
      </CONNECTION>
      <SA>
        <WAITACK>6</WAITACK>
        <WAITPACKET>45</WAITPACKET>
      </SA>
    </DEVICE>
  </DEVICES>
  <OUTPUT>
    <CONNECT_TIMEOUT>20</CONNECT_TIMEOUT>
    <SSL>ON</SSL>
    <SSLTYPE>TLS</SSLTYPE>
    <CHECKNAME>OFF</CHECKNAME>
    <CERTNAME />
  </OUTPUT>
</ROOT>